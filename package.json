{
  "name": "mastersel-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prepare": "husky",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "registry:build": "tsx ./scripts/build-registry.ts",
    "generate:api": "npx openapi-typescript openapi/api.json -o src/types/api.ts && cd scripts && node generate-api.js && node extract-types.js",
    "version": "auto-changelog -p && git add CHANGELOG.md",
    "changelog": "auto-changelog --template changelog-template.hbs",
    "download-api": "curl -o openapi/api.json https://test.mastersel.com/docs",
    "generate:api-docs": "npx openapi-generate-html -i openapi/api.json -o openapi/api.html --ui=redoc --theme=dark",
    "tag-and-changelog": "tag && npm run changelog",
    "type-check": "npx tsc --noEmit --watch"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.0",
    "@ngneat/falso": "^7.2.0",
